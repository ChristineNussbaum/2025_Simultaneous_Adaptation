################################################### Tables

table practice
  include practice.txt

table stimuluslist_female
  include stimuluslist_female.txt

table stimuluslist_male
  include stimuluslist_male.txt

################################################### Tasks

task practice
  keys f j
  show bitmap BlackTrail
  delay 300
  clear -1
  show bitmap FixcrGr端n
  table practice
  set $x random 1 2
  if $x == 1
    sound @1
    delay @2
    delay 1000
    sound @3
    delay @4
  fi
  if $x == 2
    sound @3
    delay @4
    delay 1000
    sound @1
    delay @2
  fi
  show bitmap ResponseTrail
  readkey @9 9999
  show bitmap BlackTrail
  delay 500

task female
  keys f j
  show bitmap BlackTrail
  delay 300
  clear -1
  show bitmap FixcrGr端n
  table stimuluslist_female
  set $x random 1 2
  if $x == 1
    sound @1
    delay @2
    delay 1000
    sound @3
    delay @4
  fi
  if $x == 2
    sound @3
    delay @4
    delay 1000
    sound @1
    delay @2
  fi
  show bitmap ResponseTrail
  readkey @9 9999
  show bitmap BlackTrail
  delay 500
  save @1 @2 @3 @4 @5 @6 @7 @8 $x KEY RT STATUS BLOCKORDER

task male
  keys f j
  show bitmap BlackTrail
  delay 300
  clear -1
  show bitmap FixcrGr端n
  table stimuluslist_male
  set $x random 1 2
  if $x == 1
