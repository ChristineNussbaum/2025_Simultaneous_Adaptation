    delay 1000
    sound @1
    delay @2
  fi
  show bitmap ResponseTrail
  readkey @9 9999
  show bitmap BlackTrail
  delay 500
  save @1 @2 @3 @4 @5 @6 @7 @8 $x KEY RT STATUS BLOCKORDER



################################################### Blocks

block Introduction
  message IntroductionSlide1
  message IntroductionSlide2
  message IntroductionSlide3
  message IntroductionSlide4
  message IntroductionSlide5

block Practice
  tasklist
    practice 4 all_before_repeat
  end
  message IntroductionSlide6

block Female_Speaker
  tasklist
    female 32 all_before_repeat
  end

block Break
  message BreakSlide

block Male_Speaker
  tasklist
    male 32 all_before_repeat
  end

block End
  message EndSlide

################################################### Blockorder

blockorder
  Introduction
  Practice
  Female_Speaker
  Break
  Male_Speaker
  End

blockorder
  Introduction
  Practice
  Male_Speaker
  Break
  Female_Speaker
  End
